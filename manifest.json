{
  "name": "MIDI Playground",
  "version": "0.1",
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
       "scripts": ["background.js"]
    }
  },
  "permissions": [ "usb" ],
  "optional_permissions": [ {"usbDevices": [ {"vendorId": 2536, "productId": 118} ] } ],
  "icons": { "128": "icon_128.png" }
}